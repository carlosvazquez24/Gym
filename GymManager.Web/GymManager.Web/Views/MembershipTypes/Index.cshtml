@model GymManager.Web.Models.MembershipTypeListModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";

}

@section HeaderRight{
    <form class="d-flex " role="search">
        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success" type="submit">Search</button>
    </form>

}


@section Head {
    <style>

        .table-wrapper {
            overflow-x: auto;
        }





    </style>
}

<div class="container">
    <h1> Memberships Types</h1>
    <div class="row">
        <div class="col-6"> </div>
        <div class="col-6">
            <a asp-action="Create" class="btn btn-outline-success float-end">Create </a>
        </div>
    </div>

    @await Html.PartialAsync("~/Views/Shared/_MembershipTypeList.cshtml", Model.Memberships)

</div>
