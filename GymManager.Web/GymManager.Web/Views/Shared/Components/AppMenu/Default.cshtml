@model GymManager.Web.Models.MenuViewModel
@using GymManager.Web.Models

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

    var orderedMenuTimes = Model.Menu.OrderBy(x => x.Order);
}


<ul class="navbar-nav me-auto mb-2 mb-md-0">

    @foreach (var item in orderedMenuTimes)
    {
        @await Html.PartialAsync("~/Views/Shared/Components/AppMenu/_UserMenuItem.cshtml", new UserMenuItemViewModel {
            MenuItem = item,
            RootLevel = true,
            CurrentPageName = Model.CurrentPageName
        })
    }
</ul>